@{
    Layout = null;
    var Account_In = ViewData["Account"] as NhanVien;
    var Account_In_Name = Account_In.Ho + " " + Account_In.Ten;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Del Lunar Manage</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen' href='~/css/ManageStyle.css'>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://code.jquery.com/ui/1.13.0/jquery-ui.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/html2pdf.js/0.10.1/html2pdf.bundle.min.js"
            integrity="sha512-GsLlZN/3F2ErC5ifS5QtgpiJtWd43JWSuIgh7mbzZ8zBps+dvLusV+eNQATqgA/HdeKFVgA5v3S/cIrLF7QnIg=="
            crossorigin="anonymous" referrerpolicy="no-referrer"></script>

    <script src="https://cdn.rawgit.com/rainabba/jquery-table2excel/1.1.0/dist/jquery.table2excel.min.js"></script>
    <script src="~/js/Manage.js"></script>
</head>
<body>
    <header class="header_admin">
        <div class="header_admin_left">
            <div>
                <input type="checkbox" id="check_menu_burger_id" checked>
                <label for="check_menu_burger_id">
                    <i class="fa fa-bars" style="font-size: 2.5rem;" id="check_menu_burger_icon_id"></i>
                </label>
            </div>
            <p id='today'></p>
            <p id='time_now_id'></p>
            <script>
                const timeElapsed = Date.now();
                const today = new Date(timeElapsed);
                document.getElementById('today').innerHTML = today.toDateString();
                const myInterval = setInterval(myTimer, 1000);
                function myTimer() {
                    const date = new Date();
                    document.getElementById("time_now_id").innerHTML = date.toLocaleTimeString();
                }
            </script>
        </div>
        <div>
            <img>
            <a class="name_of_hotel" id="hotel_name_reload_home_id">Del Lunar</a>
            <script>
                document.getElementById('hotel_name_reload_home_id').onclick = function () {
                    location.reload();
                }
            </script>
        </div>
        <div class="staff_info_field">
            <img id="avatar" src="~/assets/quanly/account_avatar.png" class="change_color_icon">
            <p id='name_nhanvien'>@Account_In_Name</p>
        </div>
    </header>

    <main>
        <div class="menu_burger" id="menu_burger_id">
            <ul class="user_type_field">
                @{
                    if ((bool)ViewData["Is_Admin"])
                    {
                        <li class="admin_option" style="display: block;"><a href="#admin_option_list_in_burger_id" id="admin_option_tab_burger_id">Quản lý</a></li>
                    }
                    else
                    {
                        <li class="admin_option" style="display: none;"><a href="#admin_option_list_in_burger_id" id="admin_option_tab_burger_id">Quản lý</a></li>
                    }
                }
                <li class="staff_option"><a href="#staff_option_list_in_burger_id" id="staff_option_tab_burger_id" class="chosen_tab_menu_burger">Nhân viên</a></li>
            </ul>
            <div class="option_list_in_burger" id="staff_option_list_in_burger_id">
                <div class="option_field_staff">
                    <div class="option_name_icon" id="option_qlp_menu_burger_id">
                        <img src="~/assets/quanly/quanlyphong.png" class="icon_item">
                        <p>Quản lý phòng</p>
                        <i class="fa fa-caret-left arrow_in_menu_burger" style="font-size:2rem"></i>
                    </div>
                    <div id="selection_field_qlp_menu_burger_id">
                        <div class="selection_name_icon" id="sdp_selection_name_icon_id">
                            <img class="icon_item" src="~/assets/quanly/sodophong.png">
                            <p>Sơ đồ phòng</p>
                        </div>
                        <div class="selection_name_icon" id="dsdatphong_selection_name_icon_id">
                            <img class="icon_item" src="~/assets/quanly/danhsachdatphong.png">
                            <p>Danh sách đặt phòng</p>
                        </div>
                        <div class="selection_name_icon" id="dshoadon_selection_name_icon_id">
                            <img class="icon_item" src="~/assets/quanly/danhsachhoadon.png">
                            <p>Danh sách hóa đơn</p>
                        </div>
                    </div>
                </div>
                <div class="option_field_staff">
                    <div class="option_name_icon" id="option_qlkh_menu_burger_id">
                        <img src="~/assets/quanly/quanlykhachhang.png" class="icon_item">
                        <p>Quản lý khách hàng</p>
                        <i class="fa fa-caret-left arrow_in_menu_burger" style="font-size:2rem"></i>
                    </div>
                    <div id="selection_field_qlkh_menu_burger_id">
                        <div class="selection_name_icon" id="dskhachhang_selection_name_icon_id">
                            <img class="icon_item" src="~/assets/quanly/danhsachkhachhang.png">
                            <p>Danh sách khách hàng</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="option_list_in_burger" id="admin_option_list_in_burger_id">
                <div class="admin_option_name_icon" id="quanlynhanvien_admin_option_name_icon_id">
                    <img src="~/assets/quanly/nhanvien_admin.png" class="icon_item">
                    <p>Nhân viên</p>
                </div>
                <div class="admin_option_name_icon" id="quanlyphong_admin_option_name_icon_id">
                    <img src="~/assets/quanly/phong_admin.png" class="icon_item">
                    <p>Phòng</p>
                </div>
                <div class="admin_option_name_icon" id="quanlyloaiphong_admin_option_name_icon_id">
                    <img src="~/assets/quanly/loaiphong_admin.png" class="icon_item">
                    <p>Loại phòng</p>
                </div>
                <div class="admin_option_name_icon" id="quanlydoan_admin_option_name_icon_id">
                    <img src="~/assets/quanly/doan_admin.png" class="icon_item">
                    <p>Đồ ăn</p>
                </div>
                <div class="admin_option_name_icon" id="quanlytinhluong_admin_option_name_icon_id">
                    <img src="~/assets/quanly/traluong_admin.png" class="icon_item">
                    <p>Trả lương</p>
                </div>
                <div class="admin_option_name_icon" id="quanlyDoanhThu_admin_option_name_icon_id">
                    <img src="~/assets/quanly/doanhthu_admin.png" class="icon_item">
                    <p>Doanh thu</p>
                </div>
            </div>
        </div>
        <div class="main_working_window">
            <div id="sdp_main_working_window_id" class="working_window_by_option">
                <div class="filter_sodophong">
                    <div class="branch_name_and_btn_book_hot_field">
                        @{
                            if (Account_In.IDChiNhanh == "HCM")
                            {
                                <p class="branch_name" value="HCM">Hồ Chí Minh</p>
                            }
                            else if (Account_In.IDChiNhanh == "HN")
                            {
                                <p class="branch_name" value="HN">Hà Nội</p>
                            }
                        }
