                            <option value="0">Trống</option>
                            <option value="1">Đã đặt</option>
                            <option value="2">Sửa chữa</option>
                            <option value="3">Đã trả</option>
                            <option value="4">Đã nhận</option>
                        </select>
                        <input id="number_room_filter_sodophong_info" type="text" placeholder="Số phòng">
                        <button id="search_for_filter_sodophong_info" type="submit"><i class="fa fa-search"></i></button>
                    </form>
