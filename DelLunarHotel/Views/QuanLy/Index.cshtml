                                            <input id="thoigiano_filter_book_hot_sdp_main_working_window_id" type="date" disabled name="thoigiano">
                                        </div>
                                        <div class="part_filter_book_hot_sdp_main_working_window">
                                            <p>đến</p>
                                            <input id="thoigianroidi_filter_book_hot_sdp_main_working_window_id" type="date" name="thoigianroidi">
                                        </div>
                                        <div class="part_filter_book_hot_sdp_main_working_window">
                                            <p>Số người ở</p>
                                            <select name="songuoio">
