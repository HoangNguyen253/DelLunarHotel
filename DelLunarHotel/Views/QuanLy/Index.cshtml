        var notification_status_iud_data_popup = document.querySelector("#notification_insert_update_delete_status_popup_container_id");
        var ok_notification_status_iud_data_popup = notification_status_iud_data_popup.querySelector("#btn_confirm_notification_insert_update_delete_status_popup");
        var close_notification_status_iud_data_popup = notification_status_iud_data_popup.querySelector("#header_close_notification_insert_update_delete_status_popup_id");
        ok_notification_status_iud_data_popup.addEventListener("click", function () {
            notification_status_iud_data_popup.classList.remove('show');
        });
        close_notification_status_iud_data_popup.addEventListener("click", function () {
            notification_status_iud_data_popup.classList.remove('show');
        })
        // Menu burger - begin
        document.getElementById("check_menu_burger_icon_id").addEventListener("click", () => {
            var t = document.getElementById("check_menu_burger_id").checked;
            if (t == true) {
                document.getElementsByClassName("menu_burger")[0].style.left = "-20.5%";
                document.getElementsByClassName("main_working_window")[0].style.marginLeft = "0";
            }
            else {
                document.getElementsByClassName("menu_burger")[0].style.left = "0";
                document.getElementsByClassName("main_working_window")[0].style.marginLeft = "20.5%";
            }
        })

        var options_burger_menu = document.querySelectorAll(".option_name_icon");
        options_burger_menu.forEach(function (option) {
            option.addEventListener("click", function (e) {
                var arrow_option_burger_menu = e.target.querySelector(".arrow_in_menu_burger");
                arrow_option_burger_menu.classList.toggle("rotate_90_icon_arrow");
            });
        });


        var options_admin_burger_menu = document.querySelectorAll(".admin_option_name_icon");
        var selections_options_burger_menu = document.querySelectorAll(".selection_name_icon");

        selections_options_burger_menu.forEach(function (selection) {
            selection.addEventListener("click", function (e) {
                selections_options_burger_menu.forEach(function (b) {
                    if (b !== e.target && b.classList.contains("chosen_selection_menu_burger")) {
                        b.classList.remove("chosen_selection_menu_burger");
                    }
                });
                options_admin_burger_menu.forEach(function (b) {
                    b.classList.remove("chosen_selection_menu_burger");
                });
                if (!e.target.classList.contains('chosen_selection_menu_burger')) {
                    e.target.classList.add("chosen_selection_menu_burger");
                }
            });
        });

        options_admin_burger_menu.forEach(function (option) {
            option.addEventListener("click", function (e) {
                options_admin_burger_menu.forEach(function (b) {
                    if (b !== e.target && b.classList.contains("chosen_selection_menu_burger")) {
                        b.classList.remove("chosen_selection_menu_burger");
                    }
                });
                selections_options_burger_menu.forEach(function (b) {
                    b.classList.remove("chosen_selection_menu_burger");
                });
                if (!e.target.classList.contains('chosen_selection_menu_burger')) {
                    e.target.classList.add("chosen_selection_menu_burger");
                }
            });
        });


        var admin_option_burger = document.getElementById("admin_option_tab_burger_id");
        var staff_option_burger = document.getElementById("staff_option_tab_burger_id");
        admin_option_burger.addEventListener("click", () => {
            admin_option_burger.classList.add('chosen_tab_menu_burger');
            staff_option_burger.classList.remove('chosen_tab_menu_burger');
        })
        staff_option_burger.addEventListener("click", () => {
            staff_option_burger.classList.add('chosen_tab_menu_burger');
            admin_option_burger.classList.remove('chosen_tab_menu_burger');
        })
        // Menu burger - end
    </script>
